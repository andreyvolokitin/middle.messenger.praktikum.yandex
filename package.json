{
  "name": "middle.messenger.praktikum.yandex",
  "version": "0.0.4",
  "description": "Learning project at Yandex Praktikum",
  "scripts": {
    "start": "nodemon --watch webpack.config.js --watch ./build-utils/webpack.development.js --exec \"webpack-dev-server --env mode=development\"",
    "clean": "rimraf \"./dist\"",
    "build": "npm run clean && webpack --env mode=production",
    "build:compress": "npm run build -- --env presets=compress",
    "build:analyze": "npm run build -- --env presets=analyze",
    "debug": "node --inspect --inspect-brk ./node_modules/webpack/bin/webpack.js",
    "debug:dev": "npm run debug -- --env mode=development",
    "debug:prod": "npm run debug -- --env mode=production",
    "test": "cross-env TS_NODE_PROJECT='./tsconfig.commonjs.json' TS_NODE_FILES=true mocha",
    "lint:css": "stylelint \"**/*.scss\" --fix",
    "lint:js": "eslint . --fix --ext .js,.ts",
    "lint": "concurrently \"npm run lint:css\" \"npm run lint:js\"",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "concurrently \"npm run lint\" \"npm run test\""
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreyvolokitin/middle.messenger.praktikum.yandex.git"
  },
  "author": "Andrey Volokitin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andreyvolokitin/middle.messenger.praktikum.yandex/issues"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "homepage": "https://github.com/andreyvolokitin/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@types/chai": "4.2.21",
    "@types/jsdom": "16.2.13",
    "@types/mocha": "9.0.0",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "autoprefixer": "10.3.1",
    "chai": "4.3.4",
    "compression-webpack-plugin": "8.0.1",
    "concurrently": "6.1.0",
    "copy-webpack-plugin": "9.0.1",
    "core-js": "3.15.2",
    "cross-env": "7.0.3",
    "css-loader": "6.2.0",
    "css-minimizer-webpack-plugin": "3.0.2",
    "eslint": "7.28.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.4.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.2",
    "husky": "7.0.1",
    "jsdom": "16.6.0",
    "mini-css-extract-plugin": "2.1.0",
    "mocha": "9.0.2",
    "nodemon": "2.0.12",
    "postcss": "8.3.0",
    "postcss-em-media-query": "4.1.0",
    "postcss-inline-svg": "github:wkillerud/postcss-inline-svg#2d87670d7118b980595aab38fab7661eee7401f5",
    "postcss-loader": "6.1.1",
    "prettier": "2.3.1",
