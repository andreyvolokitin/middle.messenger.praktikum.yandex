{
  "name": "middle.messenger.praktikum.yandex",
  "version": "0.0.1",
  "description": "Learning project at Yandex Praktikum",
  "scripts": {
    "start": "concurrently \"npm run express\" \"parcel watch static/*.hbs --no-cache\"",
    "watch": "parcel watch static/*.hbs --no-cache",
    "express": "node ./server.js",
    "build": "npm run lint && parcel build static/*.hbs --no-cache",
    "lint:css": "node_modules/.bin/stylelint \"**/*.scss\" --fix",
    "lint:js": "node_modules/.bin/eslint . --fix",
    "lint": "concurrently \"npm run lint:css\" \"npm run lint:js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andreyvolokitin/middle.messenger.praktikum.yandex.git"
  },
  "author": "Andrey Volokitin",
