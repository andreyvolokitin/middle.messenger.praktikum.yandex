    "postcss-em-media-query": "^4.1.0",
    "postcss-inline-svg": "^5.0.0",
    "prettier": "^2.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
